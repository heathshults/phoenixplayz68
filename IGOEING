	function mute()
    while true do
		if game.Workspace.Music.Playing == true then
	game.Workspace.Music.Playing = false
			end
			wait()
		end
		end
-------------------------------------------------------------------------------------------------------
function roadroller()
			if game.Players.LocalPlayer.Character.Head.Anchored == false then
local args = {
    [1] = game:GetService("Players").LocalPlayer.Character.Head,
    [2] = "rbxassetid://4997892695",
    [3] = 10,
    [4] = 3,
    [5] = 0,
}
    game:GetService("ReplicatedStorage").Taunt:FireServer(unpack(args))
wait(2.5)
local args = {
    [1] = game:GetService("Players").LocalPlayer.Character.Head,
    [2] = "rbxassetid://4997892695",
    [3] = 20,
    [4] = 20,
    [5] = 0,
}
    game:GetService("ReplicatedStorage").Taunt:FireServer(unpack(args))
    end
wait(16.9)
if game.Players.LocalPlayer.Character.Head.Anchored == false then
for i, player in ipairs(game.Players:GetChildren()) do
local args = {
    [1] = 2,
}

				game:GetService("ReplicatedStorage").RoadRollerExplosion:FireServer(unpack(args))
				for i, player in ipairs(game.Players:GetChildren()) do
local args = {
    [1] = player.Character.Humanoid,
    [2] = 300,
    [3] = 0.25,
    [4] = Vector3.new(0, 0, 0),
}

game:GetService("ReplicatedStorage").Damage13:FireServer(unpack(args))
local args = {
    [1] = 1,
}

game:GetService("ReplicatedStorage").RoadRollerExplosion:FireServer(unpack(args))
end
end
	end
	end
