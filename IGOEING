x.Name = "AfkPart"
x.CFrame = CFrame.new(-82.7108994, -16.8927021, -450.578796, -0.48252815, -0.156229407, 0.861834645, 1.66174363e-08, 0.983963788, 0.178368419, -0.87588048, 0.0860677958, -0.474790215)
