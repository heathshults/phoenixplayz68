function tpmenu()
loadstring(game:HttpGet("https://pastebin.com/raw/qRNwJb1Q",true))()
	end
-------------------------------------------------------------------------------------------------------
	
	function heal()
		local args = {
    [1] = CFrame.new(6061.357941, 1111.82825, 7.87677777719162, 0.922315776, 0.0115016177, -0.386265874, -0.00382397091, 0.999779701, 0.0206390861, 0.386418164, -1.0175586827, 1.922156513),
    [2] = 1000,
    [3] = "rbxassetid://137579113",
    [4] = 1.8,
    [5] = 0.18,
}

				game:GetService("ReplicatedStorage").Heal:FireServer(unpack(args))
	end

-------------------------------------------------------------------------------------------------------
	function autoheal()
		while true do
			if game.Players.LocalPlayer.Character.Humanoid.Health <= 75 then
	          heal()
				if Command:match(";unautoheal") then
		        break
				end
				if Command:match(";Unautoheal") then
		        break
				end
				end
			wait()
		end
		wait()
	end
	
