function killaura()
	local player = game.Players.LocalPlayer
while wait() do
for i,v in pairs(game.Players:GetPlayers()) do
if v ~= player and v.Character and v.Character:FindFirstChild("HumanoidRootPart") and player:DistanceFromCharacter(v.Character.HumanoidRootPart.Position) < 16 then
local args = {
    [1] = v.Character.Humanoid,
    [2] = CFrame.new(57.0669327, 1110.87073, -37.7617149, 0.486736655, 0.8563658, 0.172409922, -0.108142182, 0.254920036, -0.960896075, -0.866829038, 0.449058503, 0.216688141),
    [3] = 10,
    [4] = 0.25,
    [5] = Vector3.new(-9.85858059, 0.0012833057, 1.67582536),
    [6] = "rbxassetid://241837157",
    [7] = 0.075,
    [8] = Color3.new(255, 255, 255),
    [9] = "rbxassetid://260430079",
    [10] = 1.1,
    [11] = 0.44,
}

					game:GetService("ReplicatedStorage").Damage:FireServer(unpack(args))
local args = {
    [1] = v.Character.Humanoid,
}

game:GetService("ReplicatedStorage").Knock:FireServer(unpack(args))

end
end
end
	end
